<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.IcadsApp" parent="Theme.Material3.Light.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="colorPrimary">@color/dblue</item>
        <item name="colorPrimaryDark">@color/dblue</item>
        <item name="colorSurfaceContainer">@color/black</item>
        <item name="textAppearanceLabelSmall">@style/TextAppearanceSmall</item>
        <item name="textAppearanceButton">@style/TextAppearanceSmall</item>
        <item name="textAppearanceLabelMedium">@style/TextAppearanceSmall</item>
        <item name="colorOnSecondaryContainer">@color/white</item>
        <item name="colorSecondaryContainer">@color/dblue</item>
        <item name="colorOnSurfaceVariant">@color/dgrey</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="background">@color/white</item>
        <item name="android:fontFamily">@font/redhatdisplay</item>
    </style>
    <style name="TextAppearanceSmall" parent="TextAppearance.Material3.TitleSmall">
        <item name="fontFamily">@font/redhatdisplay</item>
        <item name="android:fontFamily">@font/redhatdisplay</item>
    </style>
    <style name="TextAppearanceMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="fontFamily">@font/redhatdisplay</item>
        <item name="android:fontFamily">@font/redhatdisplay</item>
    </style>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="Theme.IcadsApp" parent="Base.Theme.IcadsApp" />
</resources>